        </activity>
        <activity android:name="com.wanabut.fashion.Menu" android:label="Menu Utama"></activity>
        <activity android:name="com.wanabut.fashion.ListBarang" android:label="Daftar Barang"></activity>
        <activity android:name="com.wanabut.fashion.adminkategori_activity" android:label="Daftar Kategori"></activity>
        <activity android:name="com.wanabut.fashion.adminkategoritambah_activity" android:label="Tambah Kategori">
